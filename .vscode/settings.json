// Place your settings in this file to overwrite default and user settings.
{
  "editor.renderWhitespace": "all",
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "python.pythonPath": "/usr/local/bin/python3",
  "[python]": {
    "editor.tabSize": 4
  },
  "files.exclude": {
    "node_modules": true,
    ".cache": true,
    "**/__pycache__": true
  }
}